{
  "name": "slack-export-summary",
  "version": "1.0.0",
  "description": "A CLI tool to convert Slack export folder structure to organized markdown files by month",
  "main": "index.js",
  "bin": {
    "slack-summary": "./index.js"
  },
  "scripts": {
    "start": "node index.js",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "summarize": "node summarize.js",
    "summarize-all": "node summarize-all.js",
    "remove-testing-limit": "node remove-testing-limit.js"
  },
  "keywords": [
    "slack",
    "export",
    "markdown",
    "cli"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "commander": "^11.1.0",
    "dotenv": "^17.2.3",
    "openai": "^6.6.0"
  },
  "devDependencies": {
    "jest": "^29.7.0"
  },
  "jest": {
    "testEnvironment": "node",
    "collectCoverageFrom": [
      "index.js",
      "!**/node_modules/**"
    ],
    "coverageDirectory": "coverage",
    "coverageReporters": [
      "text",
      "lcov",
      "html"
    ]
  }
}
